{
  "name": "npmgraph",
  "version": "2.8.0",
  "type": "module",
  "author": {
    "name": "Robert Kieffer",
    "url": "http://github.com/broofa",
    "email": "robert@broofa.com"
  },
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@hpcc-js/wasm": "1.4.1",
    "d3": "6.6.2",
    "filter-altered-clicks": "1.0.1",
    "http-errors": "1.8.0",
    "md5": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semver": "7.3.5"
  },
  "description": "Visualize NPM dependency graphs (public and private!)",
  "keywords": [
    "npm",
    "dependencies",
    "dependency",
    "graph",
    "module"
  ],
  "repository": {
    "url": "https://github.com/npmgraph/npmgraph.git",
    "type": "git"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/eslint-parser": "7.13.14",
    "@parcel/babel-plugin-transform-runtime": "2.0.0-nightly.1823",
    "eslint": "7.24.0",
    "eslint-config-semistandard": "15.0.1",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-standard": "5.0.0",
    "parcel": "2.0.0-nightly.636",
    "standard-version": "9.2.0"
  },
  "scripts": {
    "start": "rm -fr .parcel-cache && parcel index.html --dist-dir docs",
    "build": "rm -fr .parcel-cache ./docs && parcel build index.html --dist-dir docs && cp CNAME docs/CNAME",
    "build:diff": "npm run build && git diff --quiet docs",
    "release": "standard-version",
    "test": "eslint ."
  }
}
